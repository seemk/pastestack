<% if @paste %>
    <% if @paste.private? %>
        <% if paste_owner(@paste) %>
            <%= render 'paste_content' %>
        <% else %>
            <%= render 'private_paste' %>
        <% end %>
    <% else %>
         <%= render 'paste_content' %>
    <% end %>
<% else %>
Not found
<% end %>
